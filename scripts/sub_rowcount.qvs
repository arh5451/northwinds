// RowCount.qvs
SUB rcount(vTableName)

    LET vRowCount = NoOfRows('$(vTableName)');
 //   LET vRowCount = NoOfRows('$(tableName)');
    TRACE Table $(vTableName) has $(vRowCount) rows loaded;
    
END SUB


